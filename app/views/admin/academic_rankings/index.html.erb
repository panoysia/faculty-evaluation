<% display_number = 0 %>
<div class="row">
  <div class="col-md-12">
    <h2 class="page-header">
      <span>Academic Rankings</span>
    </h2>
  </div>  <!-- .col-md-12 -->
</div>  <!-- .row -->
<table class="table table-bordered table-striped table-master content">
  <thead>
    <tr>
      <th>#</th>  
      <th>Rank Name</th>
      <!-- <th>Position</th> -->
    </tr>
  </thead>
  <tbody>
    <% @academic_rankings.each do |academic_rank| %>
      <tr>
        <td><%= display_number += 1 %></td>
        <td><%= academic_rank.full_name %></td>
        <!-- <td><%#= academic_rank.position %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>