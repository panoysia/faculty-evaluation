<div class="row">
  <div class="col-md-12">
    <h2 class="page-header">
      <span>Academic Rankings</span>
    </h2>
  </div>  <!-- .col-md-12 -->
</div>  <!-- .row -->
<table class="table table-bordered table-striped table-master content">
  <thead>
    <tr>
      <th>#</th>  
      <!-- <th>Rank Name</th> -->
      <!-- <th>Position</th> -->
      <th>Name</th>
      <th>Subrank</th>
      <th>Salary Grade</th>
      <th>CCE Min</th>
      <th>CCE Max</th>
      <th>QCE Min</th>
      <th>QCE Max</th>
    </tr>
  </thead>
  <tbody>
    <% @academic_rankings.each_with_index do |academic_rank, index| %>
      <tr>
        <td><%= index += 1 %></td>
        <!-- <td><%= academic_rank.full_name %></td> -->
        <!-- <td><%#= academic_rank.position %></td> -->
        <td><%= academic_rank.name %></td>
        <td><%= academic_rank.subrank %></td>
        <td><%= academic_rank.salary_grade %></td>
        <td><%= academic_rank.cce_min_points %></td>
        <td><%= academic_rank.cce_max_points %></td>
        <td><%= academic_rank.qce_min_points %></td>
        <td><%= academic_rank.qce_max_points %></td>
      </tr>
    <% end %>
  </tbody>
</table>