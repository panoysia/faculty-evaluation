<table class="table table-striped table-bordered table-master">
  <thead>
    <tr>
      <th>#</th>  
      <th>Title</th>
      <th>Name of Agency</th>
      <th>Date Passed</th>
      <th>Category</th>
      <th colspan="2">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @employee.professional_examinations.
                  each_with_index do |examination, index| %>
      <tr>
        <td><%= index += 1 %></td>
        <td><%= examination.title %></td>
        <td><%= examination.agency_name %></td>
        <td><%= examination.date_passed %></td>
        <td><%= examination.category_to_string %></td>
        <td>
          <%= link_to edit_admin_employee_professional_examination_path(@employee, examination) do %>
            <span class="glyphicon glyphicon-pencil"></span>
          <% end %>
          <%= link_to admin_employee_professional_examination_path(@employee, examination), method: :delete, data: { confirm: 'Do you really want to delete this record?' } do %>
            <span class="glyphicon glyphicon-remove"></span>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>