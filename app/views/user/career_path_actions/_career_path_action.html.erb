<tr>
  <td><%= career_path_action_counter += 1 %></td>
  <td><%= career_path_action.name %></td>
  <td><%= category_type_to_string(career_path_action.category) %></td>
  <td><%= date_format_readable(career_path_action.start_at) if career_path_action.start_at.present? %></td>
  <td><%= date_format_readable(career_path_action.end_at) if career_path_action.end_at.present? %></td>
  <td><%= career_path_action.company %></td>
  <td><%= career_path_action.description %></td>
</tr>